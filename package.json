{
  "name": "bot-of-thorne",
  "version": "0.1.0",
  "engines": {
    "node": ">=22 <23"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.7.9",
    "croner": "^9.0.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.16.3",
    "ioredis": "^5.4.1",
    "moment": "^2.30.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-console-format": "^1.0.8"
  },
  "description": "Crowne of Thorne Management Bot",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-syntax-import-assertions": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/types": "^8.18.1",
    "babel-jest": "^29.7.0",
    "discord-api-types": "^0.37.111",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest",
    "start:local": "tsc-watch --onSuccess \"node dist/index.js\"",
    "start": "npm run build-watch && node --watch dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//TKDBB84/bot-of-thorne.git"
  },
  "author": "Jeffrey Campbell",
  "license": "WTFPL",
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 120,
    "tabWidth": 2,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript",
    "endOfLine": "lf"
  }
}
